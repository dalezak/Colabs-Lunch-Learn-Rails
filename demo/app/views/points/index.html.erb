<ol class="breadcrumb">
  <li class="breadcrumb-item active mr-auto">Points</li>
</ol>
<div class="card-columns">
  <% @points.each do |point| %>
    <div class="card card mb-4 shadow-sm">
      <div class="card-body">
        <h5 class="card-title"><%= link_to point.to_s, point %></h5>
        <p class="card-text"><%= point.created_at %></p>
      </div>
    </div>
  <% end %>
</div>